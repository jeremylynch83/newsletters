            </selection>
        </content>
    </module>
    <module>
        <name>
            CT Head Checklist
        </name>
        <content>
            <h2 dont_print="true">
                Chronic pathology
            </h2>
            <multi_select label="White matter changes" askfurtherdetails="true">
                <dropdown label="Location" print_text_before="There is" print_text_after="white matter hypodensity">
                    periventricular | deep white matter | pontine | periventricular and deep white matter | generalised
                </dropdown>
                <dropdown label="Severity" print_text_before="which is non-specific but may represent a" print_text_after="burden of small vessel disease">
                    mild | moderate | severe
                </dropdown>
            </multi_select>
            <multi_select label="Atrophy" askfurtherdetails="true">
                <dropdown label="Cerebral severity" print_text_before="There is">
                    mild | moderate | severe
                </dropdown>
                <dropdown label="Cerebral location" print_text_after="cerebral atrophy.">
                    supratentorial | infratentorial | generalised
                </dropdown>
                <dropdown label="Hippocampal severity" print_text_before="There is">
                    mild | moderate | severe
                </dropdown>
                <dropdown label="Hippocampal location" print_text_after="hippocampal atrophy.">
                    bilateral | right | left
                </dropdown>
            </multi_select>
            <selection label="Bones">
                There are no skull fractures or destructive bony lesions
            </selection>
            <h2 dont_print="true">
                Checklist
            </h2>
            <multi_radio>
                <radio label="Ventricles" normaltext="The ventricles are not hydrocephalic. ">
                    Normal | Abnormal
                </radio>
                <radio label="Venous sinus hyperdensity" dont_print_normal="true">
                    Not present | Present
                </radio>
                <radio label="Basilar hyperdensity" dont_print_normal="true">
                    Not present | Present
                </radio>
                <radio label="Trauma/contusions" dont_print_normal="true">
                    Not present | Present
                </radio>
                <radio label="Orbits" dont_print_normal="true">
                    Normal | Abnormal
                </radio>
                <radio label="Old infarcts" dont_print_normal="true">
                    Not present | Present
                </radio>
                <radio label="Paranasal sinuses" dont_print_normal="true">
                    Normal | Abnormal
                </radio>
                <radio label="Clivus" dont_print_normal="true">
                    Normal | Abnormal
                </radio>
                <radio label="Pituitary" dont_print_normal="true">
                    Normal | Abnormal
                </radio>
                <radio label="Cerebellar tonsils" dont_print_normal="true">
                    Normal | Abnormal
                </radio>
                <radio label="Extracranial soft tissue" dont_print_normal="true">
                    Normal | Abnormal
                </radio>
            </multi_radio>
        </content>
    </module>
